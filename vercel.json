{
  "version": 2,
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
} 